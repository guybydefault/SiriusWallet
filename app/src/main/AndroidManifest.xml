<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.sirius.siriuswallet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SiriusWallet">
        <activity android:name=".EditOperationActivity"></activity>
        <activity android:name=".SelectOperationCategoryActivity" />
        <activity android:name=".SelectOperationTypeActivity" />
        <activity android:name=".EnterOperationSumActivity" />
        <activity android:name=".WalletInfoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>